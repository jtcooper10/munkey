{
  "name": "munkey",
  "version": "0.0.1",
  "description": "Peer-to-peer cryptographic password manager",
  "main": "bin/munkey.js",
  "scripts": {
    "build": "tsc",
    "start": "node bin/munkey.js --in-memory --shell",
    "start:alt": "node bin/munkey.js --in-memory --shell --port 8001 --rpc 5051",
    "start:disc": "node bin/munkey.js --in-memory --shell --discovery 9000",
    "start:disc:alt": "node bin/munkey.js --in-memory --shell --discovery 9001 --port 8001 --rpc 5051",
    "clean": "rm -rf bin node_modules",
    "test": "mocha --config ./.mocharc.json --extension .test.ts --recursive test/",
    "test:unit": "mocha --config ./.mocharc.json --extension .test.ts --recursive test/unit/",
    "test:integration": "mocha --config ./.mocharc.json --extension .test.ts --recursive test/integration/"
  },
  "keywords": [],
  "author": "Joshua Cooper",
  "license": "MIT",
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/bonjour": "^3.5.9",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/express": "^4.17.13",
    "@types/memdown": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.14.9",
    "@types/node-forge": "^1.0.1",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-core": "^7.0.10",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.1.3",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.0",
    "@munkey/munkey-rpc": "file:../lib/munkey-rpc/munkey-munkey-rpc-0.0.1.tgz",
    "argparse": "^2.0.1",
    "bonjour": "^3.5.0",
    "express": "^4.17.1",
    "express-pouchdb": "^4.2.0",
    "memdown": "^6.1.1",
    "node-forge": "^1.3.1",
    "pouchdb": "^7.2.2",
    "winston": "^3.3.3"
  }
}
